arm/Xutil       \
-echo "This test opens the device, flushes, allows samples to be"       \
-echo "collected for one second, then shows pru state and some samples" \
-open           \
-echo "Set P1 = r6(pinc)" \
-write 0x0064   \
-echo "Write(P1=r6) ->001X = pinc=1" \
-write 0x0015   \
-echo "Set P1=r3" \
-write 0x0034   \
-echo "Write (P1=r3) fifoSel: 0=adc,1=nco1" \
-write 0x1005   \
-flush          \
-usleep 1000000 \
-pru            \
-pru            \
-pru            \
-samp 
